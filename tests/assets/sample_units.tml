<layout id="root" direction="vertical">
  <container id="nav_container" constraint="4">
    <block id="nav_title" title="Nav" border="all">

    </block>
  </container>
  <container id="body_container" constraint="60%">
    <block id="body_block" border="none">
      
      <layout id="content_info" direction="horizontal">
        <container id="ats_container" constraint="5max">
          <block id="ats_block" title="Ats" border="all">
      
          </block>
        </container>
        <container id="cnt_container" constraint="60%">
          <block id="cnt_block" title="Cnt" border="all">
            
            <layout id="inner_info" direction="horizontal">
              <container id="inner_container" constraint="5">
                <block id="inner_block" title="Inn" border="all">
            
                </block>
              </container>
              <container id="cnt_container" constraint="2">
                <block id="cnt_block" title="More" border="all">
                  
                </block>
              </container>
            </layout>

          </block>
        </container>
      </layout>

    </block>
  </container>
</layout>